 if [ -d $HOME/.dots ]; then
  local dotsvar1 dotsvar2 dotsvar3 dotsvar4
  dots(){ /usr/bin/git --git-dir=$HOME/.dots/ --work-tree=$HOME "${@}" 2>/dev/null}
  dotsvar1=("${dotsvar[@]/%/'}")
  dotsvar2=("${dotsvar1[@]/#/-e '}")
  dotsvar3=(grep -v "${dotsvar2[*]}")
  dotsvar4=$(dots ls-files --modified --deleted --exclude-standard | eval "${dotsvar3[*]}")
  if ! [ -z $dotsvar4 ]; then
    echo ""
    dots status -s
  else
  	return 0
  fi
fi
