# https://github.com/lmburns/dotfiles/blob/master/.config/zsh/functions/fman

command man -k . | fzf -q "$1" --prompt='man> '  --preview $'echo {} | tr -d \'()\' | awk \'{printf "%s ", $2} {print $1}\' | xargs -r man | col -bx | bat -l man -p --color always' | tr -d '()' | awk '{printf "%s ", $2} {print $1}' | MANPAGER="sh -c 'col -bx | bat -l man -p'" MANROFFOPT="-c" xargs -r man 
